{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\" \"*.{ts,tsx,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\" \"*.{ts,tsx,js,json,md}\"",
    "format:lint": "npm run format && npm run lint:fix",
    "preview": "vite preview",
    "validate": "npm run lint && npm run format:check"
  },
  "dependencies": {
    "@clregions/data": "1.0.0",
    "@tailwindcss/vite": "4.1.17",
    "axios": "1.13.2",
    "date-fns": "4.1.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "5.5.0",
    "react-router-dom": "7.9.5",
    "socket.io-client": "4.8.3",
    "tailwindcss": "4.1.17"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@types/node": "24.6.0",
    "@types/react": "19.1.16",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.4",
    "autoprefixer": "10.4.21",
    "eslint": "9.36.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.22",
    "globals": "16.4.0",
    "postcss": "8.5.6",
    "prettier": "3.3.3",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0",
    "vite": "^7.3.0"
  }
}
